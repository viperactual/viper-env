{
  "name": "viper/lab",
  "description": "Grab your environment files from ViperLab.",
  "keywords": [ "viper", "viperlab", "env", "sync" ],
  "license": "MIT",
  "authors": [
    {
      "name": "Michael NoÃ«l",
      "email": "mike@viperframe.work"
    }
  ],
  "require": {
    "php": "^7.2.9",
    "ext-zip": "*",
    "guzzlehttp/guzzle": "^6.0",
    "symfony/console": "^4.0|^5.0",
    "symfony/filesystem": "^4.0|^5.0",
    "symfony/process": "^4.2|^5.0",
    "vlucas/phpdotenv": "3.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "bin": [
    "bin/viper-lab"
  ],
  "autoload": {
    "psr-4": {
      "Viper\\ViperLab\\Console\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Viper\\ViperLab\\Console\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
